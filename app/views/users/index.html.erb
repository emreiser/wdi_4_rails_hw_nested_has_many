<h1>Users</h1>
<h2><%= link_to "See All Articles", root_path %></h2>

<h3><%= link_to "Create new User", new_user_path %></h3>

<ul>
	<% @users.each do |user| %>
		<li><h3><%= link_to user.name, user_path(user.id) %></h3><p>User since <%= user.created_at.strftime("%d %b. %Y") %></p>
		</li>
	<% end %>
</ul>